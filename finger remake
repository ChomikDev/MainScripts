	 Touch = false
FTouch = false
DTouch = true
pp = false
loopfingeranim3()
end)

local opened = false
ToggleTabButton.MouseButton1Click:Connect(function()
	opened = not opened
	TabFrame.Visible = opened
	if opened then
		updateButton(BtnBehind,"bh")
		updateButton(BtnHead,"hd")
		updateButton(BtnPp,"pipi")
		updateButton(BtnBetweenLegs,"bl")
	end
end)
    
    hrp.CFrame = hrp.CFrame * CFrame.new(0, -27.63527, 0)
    task.wait(1.5)
    hum.Health = 0
    local function loopbang()
      
      
      FoamFinger.CanCollide = false
      FoamFinger = fakebody
      cam.CameraSubject = nil
      cam.CameraSubject = targplr.Character:WaitForChild("Humanoid")
        
      if Touch == true then
        loopfingeranim2()
      elseif FTouch == true then
        loopfingeranim()
      elseif DTouch == true then
        loopfingeranim3()
      elseif pp == true then
        loopenis()
      end
    end
    
    task.spawn(loopbang)
    targplr.CharacterAdded:Connect(function()
      task.spawn(loopbang)
    end)
  end
end)

notify("Credits", "FE Finger made by gerdroid", 1)

